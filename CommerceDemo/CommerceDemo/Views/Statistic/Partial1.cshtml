@model IEnumerable<CommerceDemo.Models.Classes.EmployeeDepartment>

<div class="card-body p-0">
    <table class="table table-sm">
        <thead>
            <tr>
                <th style="width: 10px">#</th>
                <th>Employee</th>
                <th>Progress</th>
                <th style="width: 40px">Number</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var x in Model)
            {
                <tr>
                    <td>1.</td>
                    <td>@x.DepartmanId</td>
                    <td>
                        <div class="progress progress-xs">
                            <div class="progress-bar bg-warning" style="width: 55%"></div>
                        </div>
                    </td>
                    <td><span class="badge bg-danger">@x.Total</span></td>
                </tr>
            }
            
        
           
        </tbody>
    </table>
</div>